{
  "name": "sp-backend-api",
  "version": "1.0.0",
  "private": true,
  "keywords": [],
  "author": "Jeff Whelpley",
  "license": "Apache-2.0",
  "main": "_dist/app.js",
  "scripts": {
    "setup": "npm i -g typescript && npm i",
    "test": "jest --config=./test/unit/jest.unit.json",
    "test-watch": "jest --watch --config=./test/unit/jest.unit.json",
    "test-integration": "jest --runInBand --config=./test/integration/jest.integration.json --forceExit",
    "seed": "node ./cli/seed-database.js",
    "copy-schemas": "mkdir -p ./_dist/src/schemas && cp ./src/schemas/* ./_dist/src/schemas/",
    "build": "npm run clean && tsc && npm run copy-schemas",
    "build-watch": "tsc && npm run clean && npm run copy-schemas && tsc -w",
    "start": "node _dist/src/app.js",
    "start-dev": "AWS_PROFILE=sketchpoints NODE_ENV=development STAGE=LOCAL nodemon _dist/src/app.js",
    "clean": "rm -rf _dist && mkdir _dist"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.3.2",
    "@types/bcrypt": "^1.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/express-winston": "^2.4.0",
    "@types/helmet": "0.0.37",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.104",
    "@types/mime": "^2.0.0",
    "@types/mongodb": "^2.2.16",
    "@types/node": "^8.0.53",
    "@types/uuid": "^3.4.3",
    "@types/zen-observable": "^0.5.3",
    "apollo-tracing": "0.0.9",
    "faker": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.0.1",
    "nock": "^9.1.0",
    "prettier": "^1.8.2",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.0.3",
    "tslint": "^5.7.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "apollo-link": "^1.0.3",
    "apollo-server-express": "^1.2.0",
    "aws-sdk": "^2.156.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dtrace-provider": "^0.8.5",
    "express": "^4.15.5",
    "form-data": "^2.3.1",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.21.0",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mime": "^2.0.3",
    "moment": "^2.19.2",
    "mongodb": "^2.2.31",
    "nodemon": "^1.15.0",
    "parse-domain": "^1.1.0",
    "slug": "^0.9.1",
    "source-map-support": "^0.5.0",
    "uuid": "^3.1.0",
    "winston": "^3.0.0-rc0"
  }
}
